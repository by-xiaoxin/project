
.Nav{
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	width: 100%;
	height: 40px;
	position: fixed;
}
.Nav-img{
	width: 15%;
	text-align: center;
}
.Nav .Nav-img image{
	width: 70rpx;
	height: 70rpx;
	margin-top: 5rpx;
	-webkit-transform: rotateY(180deg);
	        transform: rotateY(180deg);
}
.Nav-cont{
	width: 70%;
	height: 100%;
	line-height: 80rpx;
	background-color: #eee;
	color: #ccc;
}
.Nav-cont image{
	width: 40rpx;
	height: 40rpx;
	margin-right: 10rpx;
	margin-left: 15rpx;
	vertical-align: middle;
}
.Nav-right{
	width: 15%;
	height: 100%;
	text-align: center;
}
.Nav-right image{
	width: 70rpx;
	height: 70rpx;
	margin-top: 5rpx;
}

